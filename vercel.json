{
  "version": 2,
  "name": "redink-ai-generator",
  "buildCommand": "cd frontend && pnpm build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && pnpm install",
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "PYTHONPATH": "$PWD"
  }
}